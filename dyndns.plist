<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Disabled</key>
	<false/>
	<key>Label</key>
	<string>dyndnsc-python</string>
	<key>Nice</key>
	<integer>-15</integer>
	<key>OnDemand</key>
	<false/>
	<key>ProgramArguments</key>
	<array>
		<string>/Users/bob/bin/dyndnsc.py</string>
		<string>--loop</string>
        <string>--updater-dyndns2</string>
        <string>--updater-dyndns2-hostname</string>
        <string>myhost.example.com</string>
        <string>--updater-dyndns2-userid</string>
        <string>YOUR_USERNAME</string>
		<string> --updater-dyndns2-password</string>
		<string>YOUR_SECRET_PW</string>
		<string>--detector</string>
		<string>webcheck</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>ServiceDescription</key>
	<string>python dyndns client</string>
	<key>ServiceIPC</key>
	<false/>
</dict>
</plist>
